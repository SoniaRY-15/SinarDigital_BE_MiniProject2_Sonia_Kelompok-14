{
  "name": "pet-adoption-prisma",
  "version": "1.0.0",
  "description": "Pet Adoption App upgraded with Prisma, file uploads, and full CRUD (Express.js)",
  "main": "server.js",
  "type": "commonjs",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate": "npx prisma migrate dev --name init",
    "prisma:studio": "npx prisma studio",
    "prisma:seed": "node src/prisma/seed.js",
    "seed": "node src/prisma/seed.js"
  },
  "keywords": [
    "express",
    "prisma",
    "pet",
    "adoption",
    "multer"
  ],
  "author": "Sonia Renata Yacintaman",
  "license": "IDK",
  "dependencies": {
    "@prisma/client": "^5.0.0",
    "@faker-js/faker": "^8.0.2",
    "dotenv": "^16.0.0",
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.22",
    "prisma": "^5.0.0"
  }
}
